               @*@ <asp:CalendarExtender ID="CalendarExtender2" Format="dd/MM/y"  runat="server" PopupButtonID="txtDia2" TargetControlID="txtDia2" OnClientDateSelectionChanged="checkDate"  ></asp:CalendarExtender>*@
            </div>
            <div class="form-group">
                <label asp-for="FDP" class="control-label"></label>
                <input asp-for="FDP" class="form-control" />
                <span asp-validation-for="FDP" class="text-danger"></span>
            </div>
            <div class="form-group">
               <label asp-for="Descripcion" class="control-label"></label>
                <input asp-for="Descripcion" class="form-control" />
                <span asp-validation-for="Descripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create_Paciente" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index_Paciente">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
@if (null != TempData["Fec"])
{
    <script type="text/javascript">
       alert('@TempData["Fec"]');
    </script>
}

